                if i == 0:
                    if j == 1 or j == 2 or j == 3:
                        row = row + "#"
                    if j == 0 or j == 4:
                        row = row + " "
                if i == 1 or i == 2 or i == 3 or i == 4 or i == 5:
                    if j == 0:
                        row = row + "#"
                if i == 1:
                    if j == 1 or j == 2 or j == 3 or j == 4:
                        row = row + " "
                if i == 2:
                    if j == 1 or j == 2 or j == 3 or j == 4:
                        row = row + " "
                if i == 3:
