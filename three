strings = input("Enter word in English: ").upper()

# A
for string in strings:
    if "A" in string:
        print("\n")
        for i in range(7):
            row = ""
            for j in range(5):
                if i == 0:
                    if j == 0:
                        row = row + " "
                    if j == 1 or j == 2 or j == 3:
                        row = row + "#"
                    if j == 4:
                        row = row + " "
                if i == 1 or i == 2 or i == 4 or i == 5 or i == 6:
                    if j == 0:
                        row = row + "#"
                    if j == 1 or j == 2 or j == 3:
                        row = row + " "
                    if j == 4:
                        row = row + "#"
                if i == 3:
                    if j == 0:
                        row = row + "#"
                    if j == 1 or j == 2 or j == 3:
                        row = row + "#"
                    if j == 4:
                        row = row + "#"

            print(row)

# B

    if "B" in string:
        print("\n")

        for i in range(7):
            row = ""
            for j in range(5):
                if i == 0:
                    if j == 0 or j == 1 or j == 2 or j == 3:
                        row = row + "#"
                if i == 1:
                    if j == 1 or j == 2 or j == 3:
                        row = row + " "
                    if j == 0 or j == 4:
                        row = row + "#"
                if i == 2:
                    if j == 1 or j == 2 or j == 3:
                        row = row + " "
                    if j == 0 or j == 4:
                        row = row + "#"
                if i == 3:
                    if j == 0 or j == 1 or j == 2 or j == 3:
                        row = row + "#"
                if i == 4:
                    if j == 1 or j == 2 or j == 3:
                        row = row + " "
                    if j == 0 or j == 4:
                        row = row + "#"
                if i == 5:
                    if j == 1 or j == 2 or j == 3:
                        row = row + " "
                    if j == 0 or j == 4:
                        row = row + "#"
                if i == 6:
                    if j == 0 or j == 1 or j == 2 or j == 3:
                        row = row + "#"

            print(row)

# C

    if "C" in string:
        print("\n")
        for i in range(7):
